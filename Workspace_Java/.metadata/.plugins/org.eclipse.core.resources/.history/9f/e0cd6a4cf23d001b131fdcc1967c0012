package Test;

public class Test01 {

	public static void main(String[] args) {

		char c1 = 'a';
		System.out.println(c1);
		char c2 = 'b';
		int i1 = c2;
		System.out.println(i1);

		String s1 = "aa";
		System.out.println(s1);
		String s2 = "bb";
		System.out.println(s2);
		String sumS = s1 + s2;
		System.out.println(sumS);
		
		String s3 = "1";
		String s4 = "2";
		String sumS2 = s3 + s4;
		
	}
}