package com.test04;

public class StarPrn {

	public static void main(String[] args) {
	
		prn01();
		prn02();
	
	
	
	
}	
  /* 
   * *
   * **
   * ***
   * ****
   * *****
  */

	public static void prn01() {

		for(int i=1; i<=5; i++) {
			for(int j=1; j<=i; j++) {
				System.out.print("*");
			}
			System.out.println();
		}
	}
	
	/*
	 * *****
	 * ****
	 * ***
	 * **
	 * *
	 */
	
	public static void prn02() {
		
		for(int i=5; i<=5; i--) {
			for(int j=5; j<=i; j--) {
				System.out.println("*");
			}
			System.out.println();
			}
	}
}

