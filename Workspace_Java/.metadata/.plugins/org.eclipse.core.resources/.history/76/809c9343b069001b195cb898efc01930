package com.test01;

public class Type02 {

	public static void main(String[] args) {
		
		byte b01 = 126; 
		// 변수 타입을 byte로 지정했다.
		// 처음에는 반드시 변수 타입을 지정해야 한다. 
		System.out.println(b01);
		b01 = 111; // 변수 b01의 값을 126에서 111으로 변경했다.
		System.out.println(b01);
		// 출력하면 위에서 126이 나오던 값이 111로 변경된 것을 알 수 있다.
		
		byte b02 = (byte) 128;
		System.out.println(b02);
		
		byte sumB = (byte) (b01 + b02);
		System.out.println(sumB);
		
		
	}
}
