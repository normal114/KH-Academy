package Test;

public class Test01 {

	public static void main(String[] args) {
		
		int[] a;
		a = new int[5];
		a[0] = 1;
		a[1] = 2;
		a[2] = 3;
		a[3] = 4;
		a[4] = 5;

		int[] b = new int [] {5, 4, 3, 2, 1};
		
		int[] c = {6, 7, 8, 9, 10};
		
		System.out.println(a[0]);
		System.out.println(a[0] + b[1] + c[2]);
		System.out.println(c));
		
		
		
		
	}
}